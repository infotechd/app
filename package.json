{
  "name": "app",
  "private": true,
  "version": "0.1.0",
  "description": "A Super App for managing services",
  "author": "Maidno",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://your-repo-url.com"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "start": "concurrently \"npm:start:backend\" \"npm:start:mobile\" \"npm:start:web\"",
    "start:mobile": "npm run start --workspace frontend/mobile",
    "start:web": "npm run start --workspace frontend/web",
    "start:backend": "npm run dev --workspace backend",
    "android:mobile": "npm run android --workspace frontend/mobile",
    "ios:mobile": "npm run ios --workspace frontend/mobile",
    "build:mobile": "npm run build --workspace frontend/mobile",
    "build:web": "npm run build --workspace frontend/web",
    "test:backend": "npm run test --workspace backend",
    "test:mobile": "npm run test --workspace frontend/mobile",
    "test:web": "npm run test --workspace frontend/web",
    "test": "npm run test:backend && npm run test:mobile && npm run test:web"
  },
  "workspaces": [
    "backend",
    "frontend/mobile",
    "frontend/web"
  ],
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/cookie-parser": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "babel-preset-expo": "^12.0.11",
    "concurrently": "^8.2.2",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "cookie-parser": "^1.4.7",
    "expo-router": "^4.0.20",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "stripe": "^18.0.0"
  },
  "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f"
}
